{
  "name": "@hashfund/api",
  "private":"true",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/modules/index.js",
      "types": "./src/modules/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "bun src/index.ts",
    "node:start": "node -r tsconfig-paths/register -r ts-node/register  dist/index.js",
    "start": "bun run src/index.ts",
    "worker": "bun run src/worker.ts",
    "node:worker": "node dist/worker.js",
    "migrate": "bun drizzle-kit generate && bun drizzle-kit migrate"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.22.7",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.0",
    "@hashfund/program": "*",
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.92.3",
    "@solocker/safe-bn": "^0.0.3",
    "@tsconfig/node16": "^16.1.3",
    "@tsconfig/recommended": "^1.0.6",
    "axios": "^1.7.2",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "^0.5.1",
    "fastify": "^4.26.2",
    "fs": "^0.0.1-security",
    "imagekit": "^5.0.1",
    "moment": "^2.30.1",
    "postgres": "^3.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.23.8"
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}
